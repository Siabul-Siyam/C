#include<stdio.h>
#include<string.h>
#include<math.h>
int main(){
    
    char a;
    char b ;
    char c;
   // scanf("%c %c %c", &a, &b, &c);
    
    //printf("%c %c %c", a, b, c);
    int sum = 0;
    int n = 100;
    int reminder = n;
    for(int i = 0; i< 3; i++){
        
        int quotient = reminder / 10;
         reminder = reminder % 10;
         reminder = quotient;
        sum += (pow(2,i) * reminder);
    }
    
    printf("%d", sum);
    return 0 ;
}
